   CONFIG.NUM_READ_OUTSTANDING {1} \
   CONFIG.NUM_WRITE_OUTSTANDING {1} \
 ] [get_bd_intf_pins /Servo_Up_down/s00_axi]

  # Create instance: Servo_left_right, and set properties
  set Servo_left_right [ create_bd_cell -type ip -vlnv xilinx.com:user:PWM_Generator_v1_0:1.0 Servo_left_right ]

